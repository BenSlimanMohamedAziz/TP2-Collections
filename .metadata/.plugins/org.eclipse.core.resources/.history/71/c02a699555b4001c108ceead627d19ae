package Pile;

public class testPile {

	public static void main(String[] args)
	{
		CPile p = new CPile();
		int i = 1;
		while (i <= 10)
			p.empile(new Integer(i++));
		System.out.println(p);
		while (!p.estVide())
		{
			System.out.println(p.sommet());
			p.depile();
		}
		System.out.println("aerazer");
		p.nbElements();
	}

}
